{
  "rewrites": [
    {
      "source": "/assets/:path*",
      "destination": "/public/assets/:path*"
    }
  ],
  "headers": [
    {
      "source": "/images/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
